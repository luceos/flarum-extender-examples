{
    "name": "flarum/flarum",
    "description": "Delightfully simple forum software.",
    "type": "project",
    "keywords": [
        "forum",
        "discussion"
    ],
    "homepage": "https://flarum.org/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "flarum/approval": "^1.0",
        "flarum/bbcode": "^1.0",
        "flarum/core": "^1.0",
        "flarum/emoji": "^1.0",
        "flarum/flags": "^1.0",
        "flarum/lang-english": "^1.0",
        "flarum/likes": "^1.0",
        "flarum/lock": "^1.0",
        "flarum/markdown": "^1.0",
        "flarum/mentions": "^1.0",
        "flarum/nicknames": "^1.0",
        "flarum/pusher": "^1.0",
        "flarum/statistics": "^1.0",
        "flarum/sticky": "^1.0",
        "flarum/subscriptions": "^1.0",
        "flarum/suspend": "^1.0",
        "flarum/tags": "^1.0",
        "symfony/var-dumper": "^6.2"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
